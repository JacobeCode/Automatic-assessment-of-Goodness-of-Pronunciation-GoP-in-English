relu-renorm-layer name=tdnn2 dim=1024 input=Append(0, IfDefined(-3))
fast-lstmp-layer name=lstm2 cell-dim=1024 recurrent-projection-dim=256 non-recurrent-projection-dim=256
relu-renorm-layer name=tdnn3 dim=1024 input=Append(0, IfDefined(-3))
output-layer name=output include-log-softmax=false dim=1024
